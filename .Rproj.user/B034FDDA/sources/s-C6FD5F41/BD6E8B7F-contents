\name{psd}
\alias{psd}
\title{Pool the Standard Deviation.}
\usage{
psd(sd0, sd1, r1)
}
\arguments{
\item{sd0}{A number.}

\item{sd1}{A number.}

\item{r1}{A number.}
}
\value{
The pooled standard deviation of \code{sd0} and \code{sd1}.
}
\description{
Pool the Standard Deviation.
}
\examples{
psd(2, 2, .5)
}
